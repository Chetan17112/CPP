#include <iostream>
using namespace std;
class emp
{
  public :
  static int counter;
   string name;
   public:
  emp(string n)
  { name = n;
    cout<<name<<endl;
    counter++;
    cout<<counter<<endl;
  }
};
int emp::counter;
